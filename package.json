{
  "name": "iampeterbanjo.com",
  "version": "1.0.0",
  "description": "Personal portfolio",
  "main": "index.js",
  "engines": {
    "node": "^10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iampeterbanjo/iampeterbanjo.com.git"
  },
  "homepage": "https://iampeterbanjo.com",
  "scripts": {
    "browser-open": "chromium-browser http://localhost:8080",
    "build-clean": "rm -rf ./public/build",
    "build": "npm run blog-build && npm run css-build && npm run images-build",
    "css": "yarn --cwd css",
    "css-build": "yarn css build",
    "css-watch": "yarn css watch",
    "blog": "yarn --cwd blog",
    "blog-build": "yarn blog build",
    "blog-dev": "yarn blog dev",
    "dev": "npm-run-all -l -p blog-dev server-watch css-watch",
    "images-build": "npx gulp images",
    "images-watch": "npx gulp images-watch",
    "postinstall": "npm run build",
    "prestatic-build": "npm run static-lint && npm run static-clean",
    "server-start": "node ./server/index.js",
    "server-watch": "nodemon --inspect ./server/index.js",
    "start": "npm run server-start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "personal",
    "portfolio",
    "hapi"
  ],
  "author": {
    "email": "me@iampeterbanjo.com",
    "name": "Peter Banjo"
  },
  "license": "ISC",
  "dependencies": {
    "gatsby": "^2.1.4",
    "good": "^8.1.2",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "got": "^9.6.0",
    "hapi": "18.x.x",
    "inert": "^5.1.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.7.x",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.1",
    "autoprefixer": "^9.4.7",
    "ejs-lint": "^0.3.0",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "precss": "^4.0.0",
    "tachyons-sass": "^4.9.5"
  }
}
